    agent any
    
    stages {
        stage ('checkout') {
            steps {
                echo 'hello world'
            }
        }
    stage ('build') 
        {    
          steps {
              echo 'hello '
          }
    }
    stage ('deploy')
          {
              steps {
                  echo 'here '
              }
          }
    }  
}      
